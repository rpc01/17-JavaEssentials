import java.util.DoubleSummaryStatistics;
import java.util.Scanner;

public class Main {

	
	static String[] months = {"January", "February", "March",
            "April", "May", "June",
            "July", "August", "September",
            "October", "November", "December"};

    public static void main(String[] args) {
        loopIt("Months of the year");
        loopIt("Second run");
        loopIt("Third run");
    }
    // Reusable Code loopit to be used anywhere
    // static (to be caled from anywhere)
    // void cause does not return anything
    //name of method LoopIt
    // Will pass parameter call label which is a String
    static void loopIt(String label) {
    	System.out.println("");
        System.out.println(label);
        for (int i = 0; i < label.length(); i++) {
            System.out.print("*");
        }
        //add a blanlÃ± line
        System.out.println("");

        for (int i = 0; i < months.length; i++) {
            System.out.println(months[i]);
        }
    }
         
   
}